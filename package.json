{
  "name": "express-local-library",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "bash-server": "SET DEBUG=express-local-library:* & npm run dev"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "overrides": [
      {
        "files": [
          "*.ejs"
        ],
        "options": {
          "formatOnSave": false
        }
      }
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "luxon": "^3.4.3",
    "mongoose": "^7.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
